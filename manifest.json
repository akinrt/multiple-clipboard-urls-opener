{
    "name": "Multiple clipboard URLs Opener",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Open multiple clipboard URLs in a tab at once.",
    "permissions": [
        "clipboardRead",
        "tabs"
    ],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": {
            "64": "images/icon64.png",
            "128": "images/icon128.png"
        },
        "default_title": "Multiple clipboard URLs Opener",
        "default_popup": "popup.html"
    }
  }
